// ConsoleApplication12.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include <string>

using namespace std; 


int main()
{
	struct StudentInfo {
		string name;
		int grade;


	};

	StudentInfo student1;
	student1.name = "Geom";
		student1.grade = 1000000;

		cout << "Best student in the skewl" << endl;

		cout << "Name : " << student1.name << endl;
		cout << "Grade : " << student1.grade << "%" << endl;



    return 0;
}

