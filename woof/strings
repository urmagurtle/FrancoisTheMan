#include "stdafx.h"
#include <iostream>
#include <string>
using namespace std;


/* this program is worth 60 million $,
please give me money*/

int main()
{
	string part1 = "meow";
	string part2 = "4";
	string part3 = "pow";

	cout << part1 << " " << part2 << " " << part3 << endl;

	string wholename = part1 + part2 + part3;

	cout << wholename << endl;

	int n = wholename.length();

	cout << n << endl;


	string sub = wholename.substr(0, 3); // 0 is the position it starts at. So it starts a "m" of "meow" (m0 e1 o2 w3) and 3 is the length of the string

	cout << sub << endl;


	return 0;
}


	return 0;
}
